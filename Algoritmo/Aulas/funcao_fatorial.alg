// Seção de Declarações 
var
   Num, Res: Inteiro
Funcao fatorial(A: Inteiro): Inteiro
   escreva("Chegou aqui")
   retorne 1
FimFuncao
inicio
// Seção de Comandos
   Escreva("Digite um número: ")
   Leia(Num)
   Res <- fatorial(Num)
