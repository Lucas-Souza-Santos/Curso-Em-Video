algoritmo "exercicio 5"
// Função : Exercício 5
// Autor : Lucas de Souza Santos
// Data : 26/11/2020
// Seção de Declarações
var
   nota1, nota2, media: real
inicio
// Seção de Comandos 
   Escreva("Nota 1: ")
   Leia(nota1)
   Escreva("Nota 2: ")
   Leia(nota2)
   
   media <- (nota1 + nota2) / 2
   
   Escreval("A média entre ", nota1, " e ", nota2, " é igual a ", media)
   
fimalgoritmo
