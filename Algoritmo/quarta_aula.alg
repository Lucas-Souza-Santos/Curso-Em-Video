algoritmo "Operador Relacional"
// Função : comparar
// Autor : Lucas de Souza Santos
// Data : 25/11/2020
// Seção de Declarações 
var
   A, B, C: Inteiro
inicio
// Seção de Comandos
   A <- 2
   B <- 3
   C <- 5
   Escreval(A > B)
   Escreval(A = C)
   Escreval(C >= B)
   Escreval(C <> A)
   Escreval(B <= A)
fimalgoritmo
